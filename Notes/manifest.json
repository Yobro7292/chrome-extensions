{
  "manifest_version": 3,
  "name": "Notes",
  "version": "1.0.1",
  "description": "Just taking simple notes.",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["index.js"]
    }
  ],
  "action": {
    "default_title": "Take some notes."
  }
}
